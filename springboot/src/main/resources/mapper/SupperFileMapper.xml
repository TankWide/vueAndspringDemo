<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.SupperFileMapper">

    <insert id="insertFile" parameterType="com.example.demo.dao.entity.SupplierFile">
        INSERT INTO SUPPER_FILE(ATTACHMENT_PATH, FILE_NAME)
        VALUES (#{attachmentPath}, #{fileName})
    </insert>
    <select id="getUrl" resultType="com.example.demo.dao.entity.SupplierFile">
        select id, ATTACHMENT_PATH AS attachmentPath, FILE_NAME AS fileName
        from SUPPER_FILE
        where id = 1
    </select>

</mapper>
